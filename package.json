{
	"name": "votacion-2020-app",
	"version": "1.0.0",
	"description": "NodeJS BE challenge 2020 - git repository:",
	"main": "server.js",
	"scripts": {
		"mongo": "mongod",
		"gulp": "gulp",
		"dev": "cross-env NODE_ENV=development ts-node-dev ./src/server.ts",
		"git-add": "git add .",
		"git-commit": "git commit -m \"Mensaje: \"",
		"git-remote": "git remote add origin https://github.com/corbaz/avalith.git'",
		"git-push": "git push --set-upstream origin master",
		"dist": "npm run build && npm run copyfiles &&  npm run start",
		"build": "tsc",
		"copyfiles": "cp -r ./src/views ./dist && cp -r ./src/public ./dist && cp .env.production ./dist  &&  cp ./src/index.html ./dist",
		"start": "node ./server.js",
		"start-prueba": "cd dist && cross-env NODE_ENV=production node ./server.js",
		"bundle-webpack": "npx webpack",
		"polymer-cli": "polymer serve",
		"sass": "node-sass -w ./src/public/sass/ -o ./src/public/css/"
	},
	"keywords": [
		"Avalith",
		"challenge"
	],
	"author": "Julio Cesar Corbaz - julio.corbaz@gmail.com",
	"license": "ISC",
